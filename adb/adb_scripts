adb shell am broadcast -a ru.bellintegrator.android_at_lesson_2.CUSTOM_EVENT --es event_data "SHOW_TOAST"

adb shell am broadcast -a ru.bellintegrator.android_at_lesson_2.CUSTOM_EVENT --es event_data "OPEN_ACTIVITY"

adb shell settings get system screen_brightness

adb shell content query --uri content://contacts/phones/

# открытие диплинка
adb shell am start -a android.intent.action.VIEW -d "deeplinkexample://deeplinkactivity/open"

# открытие активити
adb shell am start -n ru.bellintegrator.android_at_lesson_2/.activity.MainActivity
